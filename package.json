{
  "name": "@roketo/dapp",
  "version": "0.2.0",
  "homepage": "https://app2.roke.to/",
  "private": true,
  "engines": {
    "node": "16.x || 18.x"
  },
  "scripts": {
    "start": "vite",
    "start:prod": "serve -l 3000 dist/",
    "build": "vite build",
    "test": "cypress run",
    "lint": "eslint -c .eslintrc src --ext .js,.jsx,.ts,.tsx",
    "check:unused:exports": "ts-unused-exports ./tsconfig.json --allowUnusedTypes --ignoreFiles=generated",
    "deploy": "yarn build && gh-pages -d build",
    "preview": "vite preview --port 3000",
    "build-styleguidist": "styleguidist server",
    "prepare": "is-ci || husky install",
    "pretty": "prettier \"./**/**.{ts,tsx,js,jsx,mjs,cjs,css,scss,sass,md,*.json,.prettierrc,.lintstagedrc,.babelrc,.eslintrc,*.webmanifest}\"",
    "format": "yarn pretty --write",
    "format:check": "yarn pretty --check",
    "generate:astro-api": "swagger-typescript-api --path https://api.testnet.app.astrodao.com/docs-json --output src/shared/api/astro/generated --name astro-api.ts --responses",
    "generate:rb-api": "swagger-typescript-api --path http://localhost:4000/docs-json --output src/shared/api/rb/generated --name rb-api.ts --responses",
    "test:e2e": "playwright test",
    "test:e2e-report": "playwright show-report"
  },
